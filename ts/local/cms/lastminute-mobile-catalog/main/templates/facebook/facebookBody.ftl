<div class="content-box">
  <div class="heading">
    <h2 class="ico2">Retrouvez-nous sur Facebook</h2>
  </div>

  <div id="fb-root"></div>
  <script>
    (function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/fr_FR/all.js#xfbml=1&appId=199458153456782";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
  </script>

  <div class="info-section">
    <div class="fb-like"
         data-href="https://www.facebook.com/lastminute.comFrance"
         data-send="false"
         data-width="450"
         data-show-faces="true"
         data-font="arial">
    </div>
  </div>
</div>